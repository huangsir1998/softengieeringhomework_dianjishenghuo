+                case "article":
+                    request.getSession().setAttribute("lastURI", request.getRequestURI());
+                    request.getRequestDispatcher("/article").forward(request, response);
+                    break;
+                case "profile":
+                    request.getRequestDispatcher("/profile").forward(request, response);
+                    break;
+            }
+        }
